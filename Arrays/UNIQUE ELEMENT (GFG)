class Solution {
  public:
    int findUnique(int k, vector<int>& arr) {
        int ans = 0;

        for (int bit = 0; bit < 32; bit++) {
            int count = 0;
            for (int num : arr) {
                if (num & (1 << bit)) {
                    count++;
                }
            }
            if (count % k != 0) {
                ans |= (1 << bit);
            }
        }
        return ans;
    }
};
