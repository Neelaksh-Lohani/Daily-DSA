class Solution {
public:
    int removeDuplicates(vector<int>& arr) {
        int i = 1;
        int j = 0;
        while(i<arr.size()){
            if(arr[i]==arr[j]){
                i++;
            }
            else{
                j++;
                arr[j]=arr[i];
                i++;
            }
        }
        return j+1;
    }
};
