// User function Template for C++
class Solution {
  public:
    pair<int, int> getMinMax(vector<int> arr) {
        // code here
        int maxAns = INT_MIN;
        int minAns = INT_MAX;
        
        for(int i = 0; i<arr.size(); ++i){
            maxAns = max(maxAns,arr[i]);
            minAns = min(minAns,arr[i]);
        }
        return {minAns,maxAns};
        
    }
};
