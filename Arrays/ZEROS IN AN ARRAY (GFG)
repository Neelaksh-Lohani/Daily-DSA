// User function template for C++

class Solution {
  public:
    int countZeroes(vector<int> &arr) {
        // code here
        int zeroCt = 0;
        int oneCt = 0;
        for(int i = 0; i<arr.size(); ++i){
            if(arr[i]==0){
                zeroCt++;
            }
            if(arr[i]==1){
                oneCt++;
            }
        }
         return zeroCt;
        
    }
};
